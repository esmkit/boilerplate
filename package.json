{
  "name": "@esmkit/boilerplate",
  "version": "0.1.0",
  "license": "MIT",
  "description": "@esmkit/boilerplate",
  "author": "Billgo <hi@billgo.me>",
  "bugs": "https://github.com/esmkit/boilerplate/issues",
  "repository": "git+ssh://git@github.com:esmkit/boilerplate.git",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      },
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      }
    },
    "./*": "./*"
  },
  "scripts": {
    "prepublishOnly": "bun run build",
    "build": "bunx rimraf dist && bunx tsup",
    "start": "bun run --watch examples/index.ts"
  },
  "dependencies": {},
  "devDependencies": {
    "@biomejs/biome": "2.3.8",
    "@devjskit/biome-config": "^0.2.1",
    "@types/bun": "^1.3.4",
    "@types/node": "^24.10.2",
    "@types/web": "^0.0.299",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  }
}
